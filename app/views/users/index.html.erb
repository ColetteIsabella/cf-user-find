<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th> Name: </th>
      <th> Email: </th>
      <th> Number: </th>
      <th> State: </th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.license.number %></td>
        <td><%= user.license.state %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to "New User", new_user_path %>
